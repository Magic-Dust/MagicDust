<item>
<title>Live Sports</title>
<link>$doregex[makelist]</link>
<regex>
<name>makelist</name>
<listrepeat><![CDATA[
<title>[makelist.param2] - [makelist.param4]</title>
<link>$doregex[get-decode]|Referer=http://yoursports.stream$doregex[hoster]&amp;User-Agent=Mozilla/5.0 (Windows NT 10.0; WOW64; rv:56.0) Gecko/20100101 Firefox/56.0</link>
<link>$doregex[get-decode2]|Referer=http:$doregex[alternate]&amp;User-Agent=Mozilla/5.0 (Windows NT 10.0; WOW64; rv:56.0) Gecko/20100101 Firefox/56.0</link>
<link>http://yoursports.stream$doregex[get-decode2]|Referer=http://yoursports.stream$doregex[hoster]&amp;User-Agent=Mozilla/5.0 (Windows NT 10.0; WOW64; rv:56.0) Gecko/20100101 Firefox/56.0</link>
]]></listrepeat>
<expres><![CDATA[<a href="([^"]*)" title="([^"]*)[^;]*;">([^<]*)[^=]*[^>]*>([^<]*)]]></expres>
<page>http://yoursports.stream/</page>
</regex>
<regex>
<name>hoster</name>
<expres>100% src="(\/i[^"]*)</expres>
<page>http://yoursports.stream/[makelist.param1]</page>
</regex>
<regex>
<name>alternate</name>
<expres>transparency src="([^"]*)</expres>
<page>http://yoursports.stream$doregex[hoster]</page>
</regex>
<regex>
<name>primary</name>
<expres><![CDATA[mustave = atob.'(.*?)']]></expres>
<page>http://yoursports.stream$doregex[hoster]</page>
<cookieJar></cookieJar>
</regex>
<regex>
<name>secondary</name>
<expres><![CDATA[atob\('([^']*)]]></expres>
<page>http:$doregex[alternate]</page>
<cookieJar></cookieJar>
</regex>
<regex>
<name>get-decode</name>
<expres>$pyFunction:base64.b64decode("$doregex[primary]")</expres>
<page></page>
</regex>
<regex>
<name>get-decode2</name>
<expres>$pyFunction:base64.b64decode("$doregex[secondary]")</expres>
<page></page>
</regex>
</item>

<item>
<title>TV Channels</title>
<link>$doregex[makelist]</link>
<regex>
<name>makelist</name>
<listrepeat><![CDATA[
<title>[makelist.param1]</title>
<link>$doregex[get-decode]|Referer=http://yoursports.stream/ing/[makelist.param2]&amp;User-Agent=Mozilla/5.0 (Windows NT 10.0; WOW64; rv:56.0) Gecko/20100101 Firefox/56.0</link>
]]></listrepeat>
<expres><![CDATA[{chan:'([^']*).[^']*.(.[^']*).[^:]*.[^:]*.[^:]*..([^']*)]]></expres>
<page>$doregex[source]</page>
</regex>
<regex>
<name>source</name>
<expres><![CDATA[src="(http:\/\/yrsprts.xyz[^"]*)]]></expres>
<page>http://yoursports.stream/</page>
</regex>
<regex>
<name>base64</name>
<expres><![CDATA[mustave = atob.'(.*?)']]></expres>
<page>http://yoursports.stream/ing/[makelist.param2]</page>
<cookieJar></cookieJar>
</regex>
<regex>
<name>get-decode</name>
<expres>$pyFunction:base64.b64decode("$doregex[base64]")</expres>
<page></page>
</regex>
</item>